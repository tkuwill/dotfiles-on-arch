! cursor setting
Xcursor.theme: ComixCursors-Orange
Xcursor.size: 48


! urxvt name
URxvt*termName: rxvt-unicode


! Disabled keycap picture insert mode and basic ISO 14755 mode
URxvt.iso14755: false
URxvt.iso14755_52: false

! Belling of urxvt
urxvt*urgentOnBell: true

! 输出时不滚动
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! 按键按下时回到最底部
URxvt*scrollTtyKeypress: true

! Fonts
 URxvt.font: xft:SauceCodeProNerdFont:bold:size=20:antialias=true,xft:Noto Sans Mono CJK TC:bold:size=24:antialias=true,xft:Symbola:size=24:antialias=true

! Fonts spacing
 URxvt.letterSpace: 1.8


! scrollbar style - rxvt (default),
! plain (compact), NeXT, or xtermn
URxvt.scrollstyle: plain
URxvt.scrollBar: false

! cursor style
URxvt*cursorBlink: true

