! Configuration for xterm

XTerm.termName: xterm-256color

! Make alt work
!XTerm*eightBitInput:   false
!XTerm*eightBitOutput:  true
XTerm.vt100.metaSendsEscape: true

! Make cursor blinking
XTerm*cursorBlink: true

! Every shell is a login shell by default (for inclusion of all necessary environment variables)
XTerm*loginshell: true

! Scrollbacks
XTerm.vt100.saveLines: 14096

! Double-click to select whole URLs :D
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

! Default font and size
XTerm*faceName: SauceCodePro Nerd Font:bold
XTerm*faceSize: 24


! Colors
XTerm*foreground: rgb:ff/ff/ff
XTerm*background: rgb:22/22/22
XTerm*color0: rgb:00/00/00
XTerm*color1: rgb:ff/55/00
XTerm*color2: rgb:af/ff/00
XTerm*color3: rgb:f0/de/18
! xterm*color4: rgb:00/00/a8
XTerm*color4: rgb:00/ae/d6
XTerm*color5: rgb:aa/00/aa
XTerm*color6: rgb:00/fc/fc
XTerm*color7: rgb:f4/f4/f4
XTerm*color8: rgb:54/54/54
XTerm*color9: rgb:fc/54/54
XTerm*color10: rgb:54/fc/54
XTerm*color11: rgb:fc/fc/54
XTerm*color12: rgb:55/ff/ff
XTerm*color13: rgb:fc/54/fc
XTerm*color14: rgb:54/fc/fc
XTerm*color15: rgb:fc/fc/fc

! Right-side scrollbar
XTerm*ScrollBar: false

! Stop output to terminal from jumping down to bottom of scroll again
XTerm*scrollTtyOutput: false

XTerm*ScrollKey: true

! copy and paste to clipboard
XTerm*VT100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)

XTerm*pointerColor: red
XTerm*pointerColorBackground: red
XTerm*pointerShape: shuttle

! End configuration for xterm
