! Configuration for uxterm

UXTerm.termName: xterm-256color

UXTerm.keysym.KP_Home: \033[1~
UXTerm.keysym.KP_End:  \033[4~

! Make alt work
!UXTerm*eightBitInput:   false
!UXTerm*eightBitOutput:  true
UXTerm.vt100.metaSendsEscape: true

! Make cursor blinking
UXTerm*cursorBlink: true

! Every shell is a login shell by default (for inclusion of all necessary environment variables)
UXTerm*loginshell: true

! Scrollbacks
UXTerm.vt100.saveLines: 14096

! Double-click to select whole URLs :D
UXTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

! Default font and size
UXTerm*faceName: SauceCodePro Nerd Font:bold
UXTerm*faceSize: 22


! Colors
UXTerm*foreground: rgb:ff/ff/ff
UXTerm*background: rgb:22/22/22
UXTerm*color0: rgb:00/00/00
UXTerm*color1: rgb:ff/55/00
UXTerm*color2: rgb:af/ff/00
UXTerm*color3: rgb:f0/de/18
! xterm*color4: rgb:00/00/a8
UXTerm*color4: rgb:00/ae/d6
UXTerm*color5: rgb:aa/00/aa
UXTerm*color6: rgb:00/fc/fc
UXTerm*color7: rgb:f4/f4/f4
UXTerm*color8: rgb:54/54/54
UXTerm*color9: rgb:fc/54/54
UXTerm*color10: rgb:54/fc/54
UXTerm*color11: rgb:fc/fc/54
UXTerm*color12: rgb:55/ff/ff
UXTerm*color13: rgb:fc/54/fc
UXTerm*color14: rgb:54/fc/fc
UXTerm*color15: rgb:fc/fc/fc

! Right-side scrollbar
UXTerm*ScrollBar: false

! Stop output to terminal from jumping down to bottom of scroll again
UXTerm*scrollTtyOutput: false

UXTerm*ScrollKey: true

! copy and paste to clipboard
UXTerm*VT100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)
! End configuration for xterm

